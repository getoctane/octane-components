{
  "name": "octane-components",
  "version": "0.15.3",
  "description": "Ready-to-use React components that make it easy to integrate with Octane",
  "author": "Octane (http://getoctane.io)",
  "license": "MIT",
  "repository": "getoctane/octane-components",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "source": "src/index.tsx",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "run-s clean build:compile build:copy",
    "build:compile": "tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json",
    "build:copy": "copyfiles -V -u 1 \"src/**/*.css\" dist",
    "clean": "rimraf dist/",
    "start": "tsc --watch",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint ./src/**/*.{ts,tsx}",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate": "run-s generate:types",
    "generate:types": "openapi-typescript -c .prettierrc https://api.cloud.getoctane.io/docs/openapi.json --output src/apiTypes.ts",
    "generate:types:dev": "openapi-typescript -c .prettierrc https://api.dev.getoctane.io/docs/openapi.json --output src/apiTypes.ts"
  },
  "peerDependencies": {
    "@stripe/react-stripe-js": ">=1.16.1",
    "@stripe/stripe-js": ">=1.46.0",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@storybook/addon-actions": "^6.4.7",
    "@storybook/addon-essentials": "^6.4.7",
    "@storybook/addon-links": "^6.4.7",
    "@storybook/react": "^6.4.7",
    "@stripe/react-stripe-js": "^1.16.1",
    "@stripe/stripe-js": "^1.46.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^12.12.38",
    "@types/numeral": "^2.0.2",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^27.4.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.2",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gh-pages": "^2.2.0",
    "jest": "^27.4.0",
    "jest-fetch-mock": "^3.0.3",
    "microbundle": "^0.14.1",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^4.4.0",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "sb": "^6.3.12",
    "storybook-addon-mock": "^1.6.0",
    "ts-jest": "^27.0.7",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "numeral": "^2.0.6",
    "pluralize": "^8.0.0",
    "prop-types": "^15.7.2"
  }
}
